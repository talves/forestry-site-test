{
  "name": "hugostarter",
  "version": "2.0.0",
  "description": "Starter files for a Hugo project",
  "main": "index.js",
  "repository": "https://github.com/theNewDynamic/hugo-starter.git",
  "author": "budparr <budparr@gmail.com>",
  "license": "MIT",
  "scripts": {
    "algolia": "atomic-algolia",
    "build": "NODE_ENV=production webpack --config webpack.js",
    "build-dev": "NODE_ENV=development webpack --config webpack.js --progress --watch",
    "deploy": "yarn build && hugo --gc",
    "hugo": "hugo server --gc --disableFastRender",
    "start": "hugo server --gc --disableFastRender"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.0.1",
    "autoprefixer": "^9.0.1",
    "babel-core": "^6.26.3",
