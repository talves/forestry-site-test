    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "lazysizes": "^4.0.2",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss": "^7.0.1",
    "postcss-cli": "^6.0.0",
    "postcss-import": "^11.0.0",
    "tailwindcss": "0.6.4",
    "webpack": "^4.16.2",
    "webpack-command": "^0.4.1"
  },
  "dependencies": {}
}
